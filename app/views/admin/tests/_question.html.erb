<% @test.questions.each do |question| %>
  <tr>
    <td> <%=  question.body %> </td>
    <td> <%= link_to "Show", admin_question_path(question), class: "btn btn-secondary" %> </td>
    <td> <%= link_to "Edit", edit_admin_question_path(question), class: "btn btn-secondary" %> </td>
    <td> <%= link_to "Delete",
      admin_question_path(question),
      class: "btn btn-secondary",
      method: :delete,
      data: { confirm: "Are you sure?" }
      %> </td>
  </tr>
<% end %>
